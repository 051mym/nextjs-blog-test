model cart {
  id                     BigInt                   @id @default(autoincrement())
  is_selected            cart_is_selected         @default(true)
  cart_code              String                   @db.VarChar(255)
  type                   cart_type                @default(delivery)
  customer_id            Int
  mitra_id               Int
  mitra_store_address_id Int
  status                 String?                  @db.VarChar(50)
  date                   String?                  @db.VarChar(20)
  time                   String?                  @db.VarChar(20)
  created_at             String?                  @db.VarChar(255)
  updated_at             String?                  @db.VarChar(0)
  is_direct_checkout     cart_is_direct_checkout? @default(false)
  customer               customer                 @relation(fields: [customer_id], references: [id], onDelete: Cascade, map: "fk_cart_customer")
  mitra                  mitra                    @relation(fields: [mitra_id], references: [id], onDelete: Cascade, map: "fk_cart_mitra")
  cart_product           cart_product[]

  @@index([customer_id], map: "fk_cart_customer")
  @@index([mitra_id], map: "fk_cart_mitra")
}

model cart_product {
  id          BigInt                   @id @default(autoincrement())
  is_selected cart_product_is_selected @default(true)
  product_id  Int
  cart_id     BigInt
  quantity    Int
  note        String                   @db.Text
  date        String?                  @db.VarChar(20)
  time        String?                  @db.VarChar(20)
  status      String?                  @db.VarChar(30)
  cart_status String                   @db.VarChar(155)
  created_at  String?                  @db.VarChar(50)
  updated_at  String?                  @db.VarChar(50)
  cart        cart                     @relation(fields: [cart_id], references: [id], onDelete: Cascade, map: "fk_cart_prd_cart_id")
  product     product                  @relation(fields: [product_id], references: [id], onDelete: Cascade, map: "fk_cart_prd_prd_id")

  @@index([cart_id], map: "fk_cart_prd_cart_id")
  @@index([product_id], map: "fk_cart_prd_prd_id")
}

